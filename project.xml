<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="Vec3D" package="com.sample.vec3d" version="1.0.0" company="Company Name" />
	<app main="Main" path="Export" file="Vec3D" />
	
	<window vsync="true" fps="60" />
	<window require-shaders="true" hardware="true" depth-buffer="true" stencil-buffer="true" antialiasing="0" />
	
	<window orientation="landscape" fullscreen="true" if="mobile" />
	
	<android target-sdk-version="19" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	
	<assets path="Assets" rename="assets" exclude="*.svg"  />
	
	<icon path="assets/icon.svg" />
	
	<template path="MainActivity.java" rename="src/com/sample/vec3d/MainActivity.java" if="android" />
	
</project>